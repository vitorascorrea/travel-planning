{
    "extends": "airbnb-base",
    "env": {
      "browser": 1
    },

    "globals": {
      "angular": 1,
      "_": 1,
      "$": 1
    },

    "rules": {
      "arrow-body-style": 0,
      "camelcase": 0,
      "comma-dangle": 0,
      "consistent-return": 0,
      "func-names": 0,
      "indent": 0,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-console":0,
      "no-else-return": 0,
      "no-nested-ternary": 0,
      "no-eval": 0,
      "no-loop-func": 0,
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "no-undef": 0,
      "no-use-before-define": 0,
      "no-case-declarations": 0,
      "no-useless-concat": 0,
      "prefer-template": 0,
      "spaced-comment": 0,
      "strict": 0
    }
}
